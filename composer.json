{
    "name": "loveduckie/silverstripe-well-known-validation",
    "description": "A SilverStripe module for conveniently allowing administrators to validate the ownership of their website or domain name with other third-party services.",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Luc Shelton",
            "email": "lucshelton@gmail.com",
            "homepage": "https://www.lucshelton.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "lucshelton@gmail.com",
        "issues": "https://github.com/loveduckie/silverstripe-well-known-validation/issues"
    },
    "keywords": [
        "silverstripe",
        "well-known",
        "validation",
        "well-known-validation",
        "keybase",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0 | ^5.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LoveDuckie\\SilverStripe\\WellKnownValidation\\": "src/",
            "LoveDuckie\\SilverStripe\\WellKnownValidation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
